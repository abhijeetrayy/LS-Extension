{
  "manifest_version": 3,
  "name": "Let's see - cine app",
  "version": "1.13",
  "description": "Save watched, favorite, and watch later actions to Supabase using TMDB API",
  "permissions": [
    "activeTab",
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "*://*/*",
    "https://api.themoviedb.org/*",
    "https://your-supabase-url.supabase.co/*"
  ],
  "action": {
    "default_popup": "src/popup/popup.html",
    "default_icon": {
      "16": "icon16.png"
    }
  },
  "icons": {
    "16": "icon16.png"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "abhijeet11ray@gmail.com"
    }
  }
}